
module BB_SYSTEM (
//////////// OUTPUTS //////////
	output BB_SYSTEM_data_OutBUS,

//////////// INPUTS //////////
	input	BB_SYSTEM_CLOCK_50,
	input	BB_SYSTEM_RESET_InHigh,
	input	BB_SYSTEM_data_InBUS
);

 parameter DATAWIDTH_BUS = 8;
 parameter PRESCALER_DATAWIDTH = 23;

wire 	[DATAWIDTH_BUS-1:0] BB_SYSTEM_SPI_OutBUS_cwire;

SPI #(.SPI_DATAWIDTH(DATAWIDTH_BUS)) SPI_u0 (
	.SPI_data_InBUS (BB_SYSTEM_data_InBUS),
	.SPI_data_OutBUS (BB_SYSTEM_SPI_OutBUS_cwire)
);

assign BB_SYSTEM_data_OutBUS = BB_SYSTEM_SPI_OutBUS_cwire;

endmodule
